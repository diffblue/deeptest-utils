<settings>
  <servers>
    <server>
      <id>ossrh</id>
      <username>${env.SONATYPETOKENUSER}</username>
      <password>${env.SONATYPETOKENPWD}</password>
    </server>
  </servers>
   
  <profiles>
    <profile>
      <id>ossrh</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <gpg.executable>gpg</gpg.executable>
        <gpg.keyname>${env.GPG_KEYID}</gpg.keyname>
        <gpg.passphrase>${env.GPG_PWD}</gpg.passphrase>
      </properties>

    </profile>
  </profiles>
</settings>
